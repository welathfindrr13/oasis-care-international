{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "..",
  "testEnvironment": "node",
  "testRegex": "\\.e2e\\.spec\\.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@oasis/(.*)$": "<rootDir>/../../libs/$1/src"
  },
  "setupFilesAfterEnv": ["<rootDir>/test/jest-setup.ts"],
  "testTimeout": 180000
}
